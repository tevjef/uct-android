<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/add_courses_to_track"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_centerHorizontal="true"
    android:layout_centerVertical="true"
    tools:visibility="visible"
    android:visibility="gone"
    >

  <com.tevinjeffrey.rutgersct.ui.utils.TintImageView
      android:id="@+id/imageView2"
      android:layout_width="160dp"
      android:layout_height="160dp"
      android:layout_centerHorizontal="true"
      android:layout_centerVertical="true"
      android:tint="@color/accent_light"
      android:contentDescription="@string/content_description_large_tracking_icon"
      android:src="@drawable/ic_track_changes_large_black"
      />

  <TextView
      android:id="@+id/textView"
      android:layout_below="@+id/imageView2"
      android:layout_centerHorizontal="true"
      android:text="Add some courses to track"
      style="@style/SecondaryTextStyle"
      />
</RelativeLayout>


    